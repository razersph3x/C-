using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Laba_2
{
    class Program
    {
        static void Main(string[] args)
        {
            int x;
            Console.Write("Введите х: ");
            x = Convert.ToInt32(Console.ReadLine());
            x = x * x;
            x = x * x;
            x = x * x;
            Console.WriteLine("Значение х= {0}", x);
        }
    }
}
